#include <iostream>
using namespace std;

int main()
{

     int T   = 0;
     int sum = 0;
     cin>>T;
     int arr[T];
     for(int i = 0;i<T;i++)
     {
         cin>>arr[i];
     }
     for(int i =0;i<T;i++)
     {
         int num = arr[i];
         int rem = num % 10;
         int quotient = 0;
         while(num > 0)
         {
             num = num / 10;
             if(num != 0)
             quotient = num;
         }
         sum = rem + quotient ;
         cout<<sum<<endl;
     }
     return 0;
}
